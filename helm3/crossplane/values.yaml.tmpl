host:
  replicas: 1

  deploymentStrategy: RollingUpdate

  image:
    repository: crossplane/crossplane
    tag: %%VERSION%%
    pullPolicy: Always

  args:
  - '--debug'

  imagePullSecrets:
  - dockerhub

tenant:
  personas:
    deploy: true

clusterStacks:
  aws:
    version: master
    deploy: false
  gcp:
    version: master
    deploy: false
  azure:
    version: master
    deploy: false
  rook:
    version: master
    deploy: false
